{
  "name": "urm",
  "version": "0.0.01",
  "private": true,
  "scripts": {
    "start:dev": "concurrently \"npm:client-debug\" \"npm:tsc-w \" \" nodemon --inspect build/urm-server/server.js \" ",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "server-debug": "tsc && run-p tsc-w server-reload",
    "client-debug": "npm start --prefix urm-client",
    "client-build": "npm run build --prefix urm-client",
    "debug": "npm-run-all --parallel client-debug server-debug",
    "tsc-w": "tsc -w"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "sybase": "^1.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.9",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  }
}
